{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/golden_sword"
    },
    "cases": [
      {
        "when": "flaming_sword",
        "model": {
          "type": "model",
          "model": "item/flaming_sword"
        }
      },
      {
        "when": "holy_blade",
        "model": {
          "type": "model",
          "model": "item/holy_sword"
        }
      }
    ]
  }
}
